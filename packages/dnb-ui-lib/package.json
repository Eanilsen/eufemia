{
  "name": "dnb-ui-lib",
  "description": "DNB UI Library",
  "license": "SEE LICENSE IN LICENSE FILE",
  "author": "DNB Team & Tobias HÃ¸egh",
  "homepage": "https://eggsdesign.github.io/dnb-design-system/uilib",
  "repository": {
    "type": "git",
    "url": "https://github.com/eggsdesign/dnb-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/eggsdesign/dnb-design-system/issues"
  },
  "version": "1.0.8",
  "main": "index.js",
  "bin": {
    "dnb-ui-lib": "./bin/dnb-ui-lib"
  },
  "scripts": {
    "cdn": "live-server --no-browser --cors --port=8081",
    "precommit": "lint-staged -d",
    "precommit:direct": "yarn prettier:all && yarn lint:fix && yarn test:staged",
    "figma": "yarn figma:update && yarn prepublish:local",
    "figma:dev": "nodemon --exec 'babel-node ./scripts/figma/updateIcons' --ignore '*.json'",
    "figma:server": "nodemon -w server --exec 'babel-node server/run' --ignore '*.json'",
    "figma:tests": "nodemon --exec 'babel-node ./scripts/tools/figmaTests' --ignore '/dist/**' --ignore '*.json'",
    "figma:update": "babel-node ./scripts/figma/updateData -- -u",
    "generate-components:dev": "nodemon --exec 'yarn babel-node ./scripts/prepub/tasks/prepareTemplates.js' -e js -w './scripts/prepub/tasks/prepareTemplates.js'  -w '**/index_template.js'",
    "generate-styles:dev": "nodemon --exec 'babel-node ./scripts/prepub/tasks/styleFactory.js' -e js -w './scripts/prepub/tasks/styleFactory.js'",
    "icons:dev": "nodemon --exec 'babel-node ./scripts/tools/convertIcons' --ignore '/dist/**' --ignore '*.json'",
    "postinstall:disabled": "cp ../../node_modules/sanitize.css/sanitize.css ./src/style/core/_sanitize.scss",
    "lint": "eslint --quiet ./src ./scripts ./stories",
    "lint-staged": "lint-staged",
    "lint:all": "yarn lint:fix && yarn lint:style",
    "lint:fix": "yarn lint --fix",
    "lint:style": "stylelint ./src/components/**/*.{js,css} ./src/patterns/**/*.{js,css} ./src/style/**/*.{js,css}",
    "prepare:disabled": "yarn startup",
    "publish": "yarn prepublishOnly && yarn publish:prerelease && yarn publish ./dnb-ui-lib-v*.tgz --new-version",
    "prepublish:dev": "nodemon -x 'babel-node ./scripts/prepub/dev.js' -w 'rollup.config.js' -e js,html,json,css,scss -w './src/style/**/*' -w './scripts/**/*' --ignore '*.json'",
    "prepublish:local": "babel-node ./scripts/prepub/runPrepub.js && yarn lint:all && yarn prettier:all",
    "publish:prerelease": "yarn pack && babel-node ./scripts/release",
    "prepublish:update": "babel-node ./scripts/prepub/updateAndPrepub.js && yarn lint:all && yarn prettier:all",
    "prepublishOnly": "yarn prepublish:update && jest --ci",
    "startup": "yarn prepublish:local",
    "story": "start-storybook -s / -p 3002",
    "story:build": "build-storybook",
    "t": "jest --watch --testPathPattern ",
    "test": "jest --watch",
    "test:screenshots": "cross-env NODE_ENV=test TAKE_SCREENSHOTS=true jest --watch --forceExit",
    "test:staged": "jest --bail --findRelatedTests",
    "test:update": "jest --updateSnapshot",
    "prettier:all": "yarn prettier:package && yarn prettier:scss && yarn prettier:fix",
    "prettier:diff": "prettier --config .prettierrc --list-different '{config,src}/**/*.js'",
    "prettier:fix": "prettier --loglevel warn --config .prettierrc --write '{scripts,stories,src}/**/*.{js,md}' --write '.babelrc'",
    "prettier:package": "prettier-package-json --write ./package.json",
    "prettier:scss": "prettier --loglevel warn --parser scss --config .prettierrc --write 'src/**/*.scss'",
    "umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/prepub/rollup.config.js",
    "umd:dev": "nodemon -x 'yarn umd' -e js,html,json,css,scss -w './src/components/**/*' -w './src/patterns/**/*' -w './src/style/**/*' -w './scripts/**/*' --ignore '*.json'"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "keycode": "^2.2.0",
    "prop-types": "^15.6.2",
    "react-text-mask": "^5.4.3",
    "sass-variable-parser": "^1.2.1",
    "what-input": "^5.1.2"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/react": "^3.4.11",
    "@svgr/core": "^3.1.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "color": "^3.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "css-vars-ponyfill": "^1.11.3",
    "del": "^3.0.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "figma-js": "^1.5.1",
    "file-older-than": "^1.0.0",
    "files-exist": "^1.1.0",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-clone": "^2.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-file": "^0.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rollup-each": "^2.1.2",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-transform": "^3.0.5",
    "gulp-uglify": "^3.0.1",
    "history": "^4.7.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-axe": "^3.1.0",
    "jest-image-snapshot": "^2.6.0",
    "jest-raw-loader": "^1.0.1",
    "jest-screenshot": "^0.2.1",
    "jest-tobetype": "^1.2.0",
    "lint-staged": "^7.3.0",
    "live-server": "^1.2.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isequalwith": "^4.4.0",
    "lodash.merge": "^4.6.1",
    "luxon": "^1.4.4",
    "node-sass": "^4.9.4",
    "node-sass-once-importer": "^5.2.0",
    "nodemon": "^1.18.4",
    "ora": "^3.0.0",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^8.0.8",
    "postcss-preset-env": "^6.2.0",
    "prettier": "^1.14.3",
    "prettier-package-json": "^2.0.1",
    "puppeteer": "^1.9.0",
    "raw-loader": "^0.5.1",
    "react-docgen": "^2.21.0",
    "react-emotion": "^9.2.12",
    "react-fake-props": "^0.2.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-scss": "^0.4.0",
    "sanitize.css": "^8.0.0",
    "sass-loader": "^7.1.0",
    "scss-to-json": "^2.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "stylelint-scss": "^3.3.2",
    "svgo": "^1.1.1",
    "text-mask-addons": "^3.8.0",
    "traverse": "^0.6.6",
    "url-loader": "^1.1.2",
    "vuera": "^0.2.3"
  },
  "lint-staged": {
    "linters": {
      "*.{js,scss}": [
        "yarn test:staged"
      ],
      "*.{js}": [
        "yarn prettier:fix",
        "yarn lint:fix",
        "git add"
      ],
      "*.{js,css}": [
        "yarn prettier:all",
        "yarn lint:style",
        "git add"
      ],
      "*.{json,md}": [
        "yarn prettier:all",
        "git add"
      ]
    },
    "ignore": [
      [
        "lib/**",
        "style/**",
        "umd/**",
        "assets/**"
      ]
    ]
  }
}
