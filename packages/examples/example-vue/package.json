{
  "name": "parcel-vue-example",
  "description": "Vue example",
  "license": "SEE LICENSE IN LICENSE FILE",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnbexperience/eufemia.git"
  },
  "version": "1.0.0",
  "scripts": {
    "build": "parcel build src/index.html",
    "build:dev": "rm -rf dist && nodemon -x 'parcel build src/index.html' -e js,jsx,html,json,css -w '/src/*'",
    "serve": "live-server dist --watch --no-browser --port=23456",
    "start": "rm -rf .cache && parcel start ./src/index.html --no-autoinstall -p 2345",
    "start:fresh": "parcel start src/index.html --no-autoinstall --no-cache -p 2345"
  },
  "dependencies": {
    "dnb-ui-lib": "*",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "vue": "^2.6.9",
    "vuera": "^0.2.3"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.6.0",
    "eslint-plugin-vue": "^5.2.2",
    "live-server": "^1.2.1",
    "parcel-bundler": "^1.12.2",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.9"
  }
}
