{
  "name": "parcel-angular-1-example",
  "description": "Angular 1 example",
  "license": "SEE LICENSE IN LICENSE FILE",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnbexperience/eufemia.git"
  },
  "version": "1.0.0",
  "scripts": {
    "build": "parcel build src/index.html",
    "build:dev": "nodemon -x 'parcel build src/index.html' -e js,jsx,html,json,css -w 'src/*'",
    "serve": "live-server dist --watch --no-browser --port=12345",
    "start": "rm -rf .cache && parcel start src/index.html --no-autoinstall -p 1234",
    "start:fresh": "parcel start src/index.html --no-autoinstall --no-cache -p 1234"
  },
  "dependencies": {
    "angular": "1.7.8",
    "dnb-ui-lib": "*",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react2angular": "4.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@types/angular": "1.6.54",
    "babel-loader": "8.0.5",
    "live-server": "^1.2.1",
    "parcel-bundler": "^1.12.2",
    "prop-types": "^15.7.2"
  }
}
